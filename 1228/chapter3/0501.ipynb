      "source": [
        "import numpy as np\n",
        "\n",
        "def MSE(y, t):\n",
        "    return np.sum((y-t)**2)/t.size\n",
        "\n",
        "t = np.array([1,    2, 3,   4])    \n",
        "y1 = np.array([0.5, 1, 1.5, 2])\n",
        "\n",
        "print(\"MSE(t, y1)=\", MSE(t, y1))\n",
        "\n",
        "y2 = np.array([0.5, 1.5, 2.5, 3.5])\n",
        "print(\"MSE(t, y2)=\", MSE(t, y2))\n"
      ]
    }
  ],
